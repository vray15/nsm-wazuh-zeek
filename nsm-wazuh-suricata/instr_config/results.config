wazuh-manager = manager:.bash_history : CONTAINS : systemctl status wazuh-manager
wazuh-agent = agent:.bash_history : CONTAINS : systemctl status wazuh-agent
suricata-install = agent:.bash_history : CONTAINS : sudo systemctl status suricata
wazuh-config = agent:/var/ossec/etc/ossec.conf : CONTAINS : eve.json
rule-customize = agent:/var/lib/suricata/rules/suricata.rules : CONTAINS : dsize:>1500
hping3 = attacker:.bash_history : CONTAINS : sudo hping3 -S -p 80 -d 2000 192.168.38.3
alerting = manager:/var/ossec/logs/alerts/alerts.json : CONTAINS : hping3
