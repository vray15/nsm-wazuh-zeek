_wazuh-manager = monitor:.bash_history : CONTAINS : systemctl status wazuh-manager
_wazuh-agent = agent:.bash_history : CONTAINS : systemctl status wazuh-agent
zeek-install = agent:.bash_history : CONTAINS : sudo apt install zeek -y
zeek-config = agent:/opt/zeek/etc/networks.cfg : CONTAINS : 192.168.10.0/24
log-output = agent:/opt/zeek/share/zeek/site/local.zeek : CONTAINS : @load policy/tuning/json-logs.zeek
wazuh-config = agent:/var/ossec/etc/ossec.conf : CONTAINS : *.log
scan-port = attacker:.bash_history : CONTAINS : sudo nmap -sS 192.168.10.3
log-detected = monitor:/var/ossec/logs/alerts/alerts.json : CONTAINS : 100904
